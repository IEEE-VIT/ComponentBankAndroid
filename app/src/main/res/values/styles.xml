<resources>

    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#D86750</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">#1B394D</item>
        <item name="windowNoTitle">true</item>
        <item name="android:statusBarColor">#8c4233</item>

    </style>


    <style name="AppTheme3" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#D86750</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">#1B394D</item>
        <item name="android:statusBarColor">#8c4233</item>

    </style>
    <style name="AppTheme2" parent="Theme.AppCompat.Light.NoActionBar">

        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">#1B394D</item>
        <item name="windowNoTitle">true</item>
        <item name="android:statusBarColor">#8c4233</item>


    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar">
        <item name="colorPrimary">#D86750</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">#1B394D</item>
        <item name="android:windowTranslucentStatus">true</item>

    </style>

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light">

        <item name="colorPrimary">#D86750</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">#1B394D</item>
        <item name="android:statusBarColor">#8c4233</item>
    </style>


    <style name="TabbedTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#D86750</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">#1B394D</item>
        <item name="windowNoTitle">true</item>
        <item name="android:statusBarColor">#8c4233</item>

    </style>

</resources>
